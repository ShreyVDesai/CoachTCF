# Backend .gitignore

# Environment variables (CRITICAL!)
.env
.env.local
.env.development
.env.production
.env.test

# Dependencies
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# Build outputs
dist/
build/

# Logs
logs/
*.log

# Testing
coverage/
.nyc_output/

# Temporary files
*.tmp
tmp/
temp/

# OS files
.DS_Store
Thumbs.db

# IDE files
.vscode/
.idea/

# PM2 (process manager) files
.pm2/
ecosystem.config.js

# Database files (if using local SQLite for testing)
*.sqlite
*.sqlite3
*.db

# Uploaded files (if storing locally during development)
uploads/
public/uploads/

# SSL certificates (if stored locally)
*.pem
*.key
*.cert
*.crt

# Backup files
*.backup
*.bak
*.sql.backup

